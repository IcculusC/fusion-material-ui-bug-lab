{
  "name": "example-fusion-app",
  "version": "0.0.0",
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "fusion-cli": "^1.9.0",
    "fusion-core": "^1.5.0",
    "fusion-plugin-react-helmet-async": "^1.0.0",
    "fusion-plugin-react-router": "^1.2.0",
    "fusion-plugin-styletron-react": "^2.5.2",
    "fusion-react": "^1.2.0",
    "fusion-tokens": "^1.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "styletron-react": "^4.3.5"
  },
  "scripts": {
    "install-submodules": "yarn --cwd ./fusion-plugin-material-ui",
